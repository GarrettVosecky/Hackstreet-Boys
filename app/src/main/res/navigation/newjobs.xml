<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/newjobs"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.hackstreet_boys.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/addFragment"
        android:name="com.example.hackstreet_boys.AddFragment"
        android:label="fragment_add"
        tools:layout="@layout/fragment_add" >
        <action
            android:id="@+id/action_addFragment_to_completedJobsPage"
            app:destination="@id/completedJobsPage" />
    </fragment>
    <activity
        android:id="@+id/completedJobsPage"
        android:name="com.example.hackstreet_boys.CompletedJobsPage"
        android:label="activity_completed_jobs_page"
        tools:layout="@layout/activity_completed_jobs_page" />
</navigation>